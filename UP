package wquiqew;

public class Classeprincipal {

	public static void main(String[] args) {
		
		Funcionario bruno = new Funcionario("Bruno", "5456646");
		bruno.salario = 100;
		System.out.println(bruno.getPessoa());
		

	}
}
package wquiqew;

public class Pessoa {
	String nome;
	String cpf;
	String dataNascimento;
	Endereco endereco;
	
	public Pessoa(String nome, String cpf, String dataNascimento) {
		this.dataNascimento = dataNascimento;
		this.cpf = cpf;
		this.nome = nome;
		this.endereco = new Endereco();
	}
	
	public Pessoa() {
		this.dataNascimento = "";
		this.cpf = "";
		this.nome = "";
		this.endereco = new Endereco();
	}
	public String getPessoa() {
		return "Nome: "+this.nome+"\n"+
	           "Dt Nascimento: "+this.dataNascimento+"\n"+
				"CPF: "+this.cpf+
				"Endereco: "+this.endereco.enderecoCompleto();
	}
}
package wquiqew;

public class Funcionario extends Pessoa {
	
	double salario;
	String dataAdmissao;
	
	
	public Funcionario() {
		super();
		this.dataAdmissao = "";
		this.salario = 0;
		
		
	}
	public Funcionario(String nome, String cpf) {
		super(nome, cpf, "01/01/01");
	    this.dataAdmissao = "";
	    this.salario = 0;
		
		
	}
	
	public String getPessoa() {
		return super.getPessoa()+"\n"+
		" data de admissao: " + this.dataAdmissao +"\n"+
				"Salario: " + this.salario;
		
		
		
	}
	
	
	

}
package wquiqew;

public class Fabricante {
	
	String razaoSocial;
	String cnpj;
	Endereco endereco;

}
package wquiqew;

public class Endereco {
	String logradouro;
	String cep;
	String numero;
	String bairro;
	String cidade;
	String uf;
	
	public Endereco() {
		this.logradouro = "";
		this.cep = "";
		this.bairro = "";
		this.numero = "";
		this.uf = "";
		
		
	}
	
	public String enderecoCompleto() {
		return this.logradouro+", "+
	           this.numero+", "+
				this.bairro+" - "+
	           this.cidade+"/"+this.uf;
	}

}
package wquiqew;

public class Piloto {

	
	String cor;
	String formato;
	double tamanhoPonta;
	boolean tampado;
	int qtdCarga;
	
	public Piloto() {
		this.qtdCarga = 100;
		this.tampado = true;	
	}
	public void escrever(String texto) {
		if ((!this.tampado)&&(this.qtdCarga>0)) {
			System.out.println(texto);
		}else {
			System.out.println("Impossivel escrever");
		}
	}
	public void tampar() {
		if (!this.tampado) {
			System.out.println("Tampei a caneta");
			this.tampado = true;
		}else {
		}
	}
	public void destampar() {
		if (this.tampado) {
			System.out.println("Ja esta tampada");
			System.out.println("Destampei a caneta");
			this.tampado = false;
		}else {
			System.out.println("Ja esta destampada");
		}
	}
}
